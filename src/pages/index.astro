---
import "@/styles/globals.css";
//import Channels from "@/components/Channels.astro";
import Layout from "@/layouts/Layout.astro";
// tsx components
import { verifyToken } from "@/lib/utils";
import Main from "@/components/Main";


const cookie: any = Astro.request.headers.get("cookie");
const isValidate = await verifyToken(cookie);
if (!isValidate) {
  return Astro.redirect("/auth/login-user");
}

---

<Layout title="Chat index.">
    <Main client:only="react"/>
</Layout>
